  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src --exec 'ts-node' src/index.ts",
    "docker:dev": "docker-compose -f docker-compose.development.yml up --build",
    "docker:start": "docker-compose up -d --build",
