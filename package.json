{
  "name": "saucebot",
  "description": "Use the sauce",
  "version": "1.0.0",
  "author": "kokage <k0kag3.k0kag3+github@gmail.com>",
  "scripts": {
    "build": "tsc",
    "dev": "docker-compose -f docker-compose.development.yml up --build",
    "dev:nodemon": "nodemon --watch src --exec 'ts-node' src/index.ts",
    "docker:start": "docker-compose up -d --build",
    "docker:stop": "docker-compose down",
    "docker:update": "yarn docker:stop && git pull --rebase && yarn docker:start",
    "start": "node .",
    "test": "jest"
  },
  "main": "dist/index.js",
  "dependencies": {
    "ascii2d": "^3.0.1",
    "bytes": "^3.1.0",
    "date-fns": "^2.21.3",
    "debug": "^4.3.1",
    "discord.js": "^12.5.3",
    "jsdom": "^16.6.0",
    "node-fetch": "^2.6.7",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.23",
    "@types/jsdom": "^16.2.10",
    "@types/node": "^15.6.0",
    "@types/node-fetch": "^2.5.10",
    "@types/validator": "^13.1.3",
    "jest": "^27.0.1",
    "nodemon": "^2.0.7",
    "ts-jest": "^27.0.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
