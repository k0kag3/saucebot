    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.2.3",
    "@types/jsdom": "^16.2.3",
    "@types/node": "^14.0.6",
    "@types/node-fetch": "^2.5.7",
    "@types/validator": "^13.0.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
